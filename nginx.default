server {
    listen 8030;
    server_name 127.0.0.1;

    location / {
        proxy_pass http://127.0.0.1:8010;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
    location /static {
        root /srv/www/;
    }
    access_log /var/log/nginx/behavioral_control_access.log;
 	error_log /var/log/nginx/behavioral_control_error.log;
}
